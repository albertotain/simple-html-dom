{
    "name": "albertotain/simple-html-dom",
    "description": "simple-html-dom plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": ["cakephp3" ],
    "homepage": "https://github.com/albertotain/simple-html-dom",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "SimpleHtmlDom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleHtmlDom\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
